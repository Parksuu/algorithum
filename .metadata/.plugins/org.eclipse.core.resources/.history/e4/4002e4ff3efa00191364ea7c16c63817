import java.util.*;

public class programmers_2_예상대진표 {

    public int solution(int n, int a, int b){
        ArrayList <Integer> list = new ArrayList<Integer>();
        
        for(int i=1;i<=n;i++)
        	list.add(i);
        
        int a_idx = a;
        int b_idx = b;
        
        int answer = 0;
        while(true) {
	        for(int i=1; i <= list.size(); i = i+2) {
	        	
	        	if(arr[i] == a_idx || arr[i] == b_idx) {
	        		
	        	}
	        	if(arr[i+1] == a_idx || arr[i+1] == b_idx) {
	        		
	        	}
	        	else {
	        		
	        	}
	        	
	        }
	        
	        answer++;
	        
	        if(a_idx+1 ==b_idx) {
	        	answer = answer +1;
	        	break; //break하고 나가기
	        }
	        
        }

        return 0;
    }
	
	
