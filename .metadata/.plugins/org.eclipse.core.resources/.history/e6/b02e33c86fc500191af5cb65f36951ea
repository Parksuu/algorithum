import java.util.*;


public class programmers_2_타겟넘버 {

	public int solution(int[] numbers, int target) {
    
        ArrayList<Integer> list = new ArrayList<Integer>();
        
        //2개 일 때,
        list.add(numbers[0]);
        list.add(-1 * numbers[0]);
        
        for(int i=1;i<numbers.length; i++) {
        	int size = list.size();
        	for(int j=0; j<size; j++) {
        		int a = list.remove(j);
        	}
        	
        }
        
        
        int answer = 0; 
        return answer;
    }
    
