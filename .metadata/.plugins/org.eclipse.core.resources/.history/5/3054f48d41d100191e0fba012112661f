import java.util.*;

public class kakao_2019_1 {
	
    public int solution(String s) {
        int answer = 0;
        
        while(true) {
        	int i=1;//i개씩 끊어서 비교한다
        	for(int j=0;j<s.length();j++) {
        		String nows = s.substring(j,j+i);
        		String nexts = s.substring(j+i, (j+1) + i);
        	}
        
        }
        
        
        return answer;
    }
	
	
	public static void main(String[] args) {
		
	}
}
