import java.util.*;


public class programmers_3_네트워크{

	
    public int solution(int n, int[][] computers) {
        int answer = 0;
        boolean visited[][] = new boolean[n][]; //false로 초기화
        
        LinkedList<Integer> list = new LinkedList<Integer>(); //연결된거 저장
        
        for(int i=0; i<n; i++) {
        	for(int j=0;j<computers[i].length; j++) {
        		if(computers[i][j]==1 && visited[i][j]==false) {
        			visited[i][j] = true;
        		}
        		
        		
        	}
        }       
        
        return answer;
    }	
	
	
	public static void main(String[] args) {
		
	}
}