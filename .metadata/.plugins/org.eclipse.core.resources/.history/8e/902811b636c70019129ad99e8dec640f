import java.util.*;

public class exampleeee {

	static class pos{
		int x;
		int y;
		
		pos(int x,int y){
			this.x = x;
			this.y = y;
		}
	}
	
	public static void main(String[] args) {
		
		ArrayList<pos> list = new ArrayList<pos>();
		
		pos p = new pos(3,4);
		
		list.add(p);
		
		System.out.println(p.x +" " + "" +p.y);
	}
	
	
	
}
