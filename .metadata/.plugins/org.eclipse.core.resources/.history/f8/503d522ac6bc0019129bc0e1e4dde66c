
import java.util.*;

public class programmers_2_49993 {

    static public int solution(String skill, String[] skill_trees) {
        int answer = 0;
        ArrayList<Integer> location;
        
       for(int i=0; i<skill_trees.length; i++) {
    	   
    	   location = new ArrayList<Integer>();//한 스킬 트리 확인할 때 마다 초기화
       	   boolean check = false;
       	   
        	for(int j=0; j<skill_trees[i].length(); j++) {
        		String a =String.valueOf(skill_trees[i].charAt(j));

        		if(skill.contains(a)) 
        			location.add(skill.indexOf(a));
        		
        	}

        	
        	if(location.isEmpty()) 
        		answer = answer +1;
        		
        	else {
        		
        		for(int k=0; k<location.size(); k++) {
        			
        			if(location.get(k) == k) 
        				check = true;
        			
        			else
        				break;
        		}
        		
        	}
        	
    		if(check == true)
    			answer = answer +1;	
        }
        
        return answer;
    }
	
    
    public static void main(String[] args) {
		String skill = "CBD";
    	String s[] = {"BACDE"};
    	String sk[] = {"BACDE", "CBADF", "AECB", "BDA"};
    	
    	System.out.println(solution(skill,sk));
	}
    
	
}



