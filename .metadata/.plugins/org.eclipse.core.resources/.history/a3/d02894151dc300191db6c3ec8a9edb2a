import java.util.*;

public class beak_3190_뱀 {

	static int wall[][];

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		wall = new int[n+2][n+2]; //1부터 시작하려고
		
		//벽 ->2
		for(int i=0;i<n+2; i++) {
			for(int j=0;j<n+2; j++) {
				if(i==0 || j==0 || i==n+1 || j==n+1)
					wall[i][j] = 2; //0번째 행,열과 n+1번째 행,열을 벽으로 만들어줌
			}
		}
		
		//사과 ->1
		int k = sc.nextInt();
		for(int i=0;i<k; i++) {
			int a=sc.nextInt();
			int b = sc.nextInt();
			wall[a][b]=1;
		}
		

	}
}
