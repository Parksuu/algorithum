import java.util.*;

public class kakao_2019_1 {
	
    public int solution(String s) {
        int answer = 0;
        HashMap<String,Integer> map = new HashMap<String,Integer>();
        
        while(true) {
        	int i=1;//i개씩 끊어서 비교한다
        	for(int j=0;j<s.length();j++) {
        		int sum = 0;
        		String nows = s.substring(j,j+i);
        		String nexts = s.substring(j+i, (j+i) + i);
        		
        		if(nows.equals(nexts)) {
        			if(map.containsKey(nows)) {//이미 있는 a에 더하면
        				
        			}
        		}
        		
        	}
        	i++;
        }
        
        
        return answer;
    }
	
	
