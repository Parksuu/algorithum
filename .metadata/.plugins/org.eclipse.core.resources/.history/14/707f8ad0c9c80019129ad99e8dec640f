public class programmser_2_HIndex {

    static public int solution(int[] citations) {
        int size = citations.length;
    	
    	int arr[][] = new int[size][3];	
    	int answers[] = new int[size];
        
    	for(int i=0;i<size;i++) 
    		arr[i][0] = citations[i];
        
    	for(int i=0;i<size;i++) {
    		arr[i][1] = 1;
    	}
    	
    	
    	for(int i=0;i<size;i++) {
    		for(int j=0; j<size; j++) {
    			if(i != j && citations[i] <= citations[j]) {
    				arr[i][1] = arr[i][1] + 1;
    			}
    			if(i !=j && citations[i]> citations[j]) {
    				arr[i][2] = arr[i][2] + 1;
    			}
    		}
    	}
    	
    	
    	int answer = 0;
    	//arr[i][0]이상 인용된 논문이 arr[i][1]편 이상이고 나머지 논문이 arr[i][2]번 이하 인용
    	for(int i=0;i<size;i++) {
    		if(arr[i][0] == arr[i][1]) {
    			if(arr[i][2] <=arr[i][1]) {
        				answer = arr[i][1];
        				System.out.println(answer);
    			}
    		}
    	}
    	
    	for(int i=0; i<size; i++) {
    		System.out.print("i,0->" + arr[i][0] + "  ");
    	}
    	
    	System.out.println("");
    	for(int i=0; i<size; i++) {
    		System.out.print("i,1-> " + arr[i][1]+ "  ");
    	}
    	
        return answer;
    }
	
	
	public static void main(String[] args) {
		int a[]= {3,0,6,1,5};
		
		System.out.println(solution(a));
		
	}
}
