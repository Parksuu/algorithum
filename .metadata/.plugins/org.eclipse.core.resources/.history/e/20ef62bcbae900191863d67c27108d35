import java.util.*;

public class beak_1918_후위표기식 {

	public static void main(String[] args) {
		Stack<String> stk = new Stack<String>();
		
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		
		String arr[] = s.split("");
		ArrayList<String> list = new ArrayList<String>();
		
		for(int i=0;i<arr.length;i++) {
			String a = arr[i];
			
			if(stk.peek().equals("(")) {
				stk.push(a);
				continue;
			}
			
			
			if(a.equals("+") || a.equals("-")) {
				if(stk.isEmpty())
					stk.push(a);
				else {

				}
			}//if
			
			else if(a.equals("*") || a.equals("/")) {
				if(stk.peek().equals("*") || stk.peek().equals("/")) {
					list.add(stk.pop());
					stk.push(a);
				}
				else {
					stk.push(a);
				}
				
			}
			
			else if(a.equals("(")) {
				stk.push(a);
			}
			
			else if(a.equals(")")){
				while(!stk.peek().equals("(")) {
					String p = stk.pop();
					if(p.equals("+") || p.equals("-") || p.equals("*") || p.equals("/"))
						list.add(p);
				}
				stk.pop(); //해줘야 (까지 없어짐
			}
			
			else { //a~z의 문자
				list.add(a);
			}
			
			
		}//for
		
	}
}
