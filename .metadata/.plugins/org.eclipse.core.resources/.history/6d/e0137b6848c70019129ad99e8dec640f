import java.util.*;


public class programmers_3_네트워크{

	static int computer[][];
	static boolean visited[];
	static List<Integer> list = new LinkedList<Integer>();
	static int answer = 0;
	
	static int dfs(int a[][], boolean visited[], int idx) {
		int num = 0;
		visited[idx] = true; //idx 방문
		list.add(idx);
		System.out.print("dfs?" + idx + " ");
		
		for(int i=idx; i<a[0].length; i++) {
			for(int j=num; j<a.length; j++) {//행만큼 다 돈다, 세로로
				if(a[j][i]==1 && visited[j] ==false) 
					dfs(a,visited,j); //세로로 가다가
				else if(a[j][i]!=1 && visited[j]== true) {
					answer++;
					System.out.println("한번이잖아");
					break;
				}
				System.out.println("요이?");				
			}//for
			num = num+1;
		}//for
		
		return answer;
	}
	
    static public int solution(int n, int[][] computers) {
    	computer = computers;
    	visited = new boolean[n];
    	dfs(computer,visited,0);

        return answer;
    }
	
	
	public static void main(String[] args) {
		int a[][] = {{1,1,0},{1,1,1},{0,1,1}};
		System.out.println("답 " +solution(3,a));
	}
}