import java.util.*;

public class programmers_3_µî±·±æ {

	
    public int solution(int m, int n, int[][] puddles) {
        int answer = 0;
        int dp[][] = new int[m+1][n+1];
        
        dp[1][1] = 1; //ÃÊ±â °ªÀº 1·Î
        for (int[] puddle : puddles) {
            dp[puddle[1]][puddle[0]] = -1; //¿õµ¢ÀÌ´Â -1
        }
        
        
        return answer;
    }
    
    
}
