import java.util.*;

public class beak_3190_뱀 {

	
	public static void main(String[] args) {

		int wall[][];
		ArrayList<Integer> list = new ArrayList<Integer>();
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		wall = new int[n+1][n+1]; //1부터 시작하려고
		
		//사과 ->1
		int k = sc.nextInt();
		for(int i=0;i<k; i++) {
			int a=sc.nextInt();
			int b = sc.nextInt();
			wall[a][b]=1;
		}
		
		//방향전환 2개가 pair
		
		
		
		
	}
}
