import java.util.*;

public class beak_3190_뱀 {

	static int wall[][];
	static int i=1,j=1; //뱀 진행

	static void right(int w) {
		
	}
	
	static void left(int w) {
		
	}
	
	public static void main(String[] args) {

		ArrayList<Integer> list = new ArrayList<Integer>();
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		wall = new int[n+1][n+1]; //1부터 시작하려고
		
		//사과 ->1
		int k = sc.nextInt();
		for(int i=0;i<k; i++) {
			int a=sc.nextInt();
			int b = sc.nextInt();
			wall[a][b]=1;
		}
		
		//방향전환 2개씩 pair
		int l = sc.nextInt();
		for(int i=0; i<l; i++) {
			int a = sc.nextInt();
			list.add(a);
			String s = sc.next();
			if(s.equals("D")) //D 오른쪽이면 -2 리스트에 넣는다
				list.add(-2);
			else			  //L 왼쪽이면 -1 리스트에 넣는다
				list.add(-1);
		}
		
		int sec = 1; //초
		int snk = 3; //뱀 존재하는 곳 3
		wall[1][1] =snk;
		
		int loc_sec = list.get(0); //첫 회전 시간
		list.remove(0);
		int loc ; //첫 회전 방향
				
		
		//진행
		while(true) {
			//loc이 0되면 list remove해주고 방향 바꿔주고
			if(loc==0) {
				loc = list.get(0);
				if(loc == -2)
					right(snk);
				else
					left(snk);
			}
			//부딪혔는지 아닌지 여부
			
			//사과 존재 여부
			if(wall[i][j])
			//움직이기
			
			
			loc--;
			sec++;
		}
		
		
		
		
		
		
		//출력해보는거
		for(int i=1;i<=n ;i++) {
			for(int j=1; j<=n; j++) {
				System.out.print(wall[i][j]);
			}
			System.out.println("");
		}
		
		System.out.println(list);
		
		
	}
}
